    sub alef-ar' lookup AllahSubs1 @MarksAny' @MarksAny' lam-ar.init' lookup AllahSubs1 lam-ar.medi' lookup AllahSubs3 [heh-ar.fina hehgoal-ar.fina]' lookup AllahSubs2;
