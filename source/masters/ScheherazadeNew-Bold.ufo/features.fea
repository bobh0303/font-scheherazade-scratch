# Subtending mark positioning
lookup SubtendingMarks {
    lookupflag IgnoreMarks;
    # This code is taken from Harmattan.
    # In Graphite we use mark attachment rules. However, in OpenType positioning digits on 
    # subtending marks has to be done with adjustments rather than attachment since for some 
    # engines the digits will ordered *before* the subtending mark.
    # Additionally, the adjustment value records depend on font face (bold vs regular). Therefore
    # we use feax extentions to calculate the value records based on glyph advancewidths and digit AP coordinates.
    # In each "do" template, the first rule is ordered for Uniscribe or HarfBuzz 'latin', the second for HarfBuzz 'arab'
    # U+06DD End of Ayah
            pos @AyahAny @DigitsAnySmall' <-1716 0 -468 0> @DigitsAnySmall' <-1248 0 -468 0> @DigitsAnySmall' <-780 0 -468 0>;
        pos @DigitsAnySmall' <-780 0 -468 0> @DigitsAnySmall' <-1248 0 -468 0> @DigitsAnySmall' <-1716 0 -468 0> @AyahAny;

            pos @AyahAny @DigitsAnyMedium' <-1659 0 -667 0> @DigitsAnyMedium' <-992 0 -667 0>;
        pos @DigitsAnyMedium' <-992 0 -667 0> @DigitsAnyMedium' <-1659 0 -667 0> @AyahAny;

            # U+0600 Number sign
        pos @AyahAny @DigitsAnyMedium' <-1340 0 -667 0>;
        pos @DigitsAnyMedium' <-1340 0 -667 0> @AyahAny;

            pos number-ar.3 @DigitsAnyMedium' <-2609 0 -667 0> @DigitsAnyMedium' <-1942 0 -667 0> @DigitsAnyMedium' <-1275 0 -667 0>;
        pos @DigitsAnyMedium' <-1275 0 -667 0> @DigitsAnyMedium' <-1942 0 -667 0> @DigitsAnyMedium' <-2609 0 -667 0> number-ar.3;

            pos number-ar.2 @DigitsAnyMedium' <-1994 0 -667 0> @DigitsAnyMedium' <-1327 0 -667 0>;
        pos @DigitsAnyMedium' <-1327 0 -667 0> @DigitsAnyMedium' <-1994 0 -667 0> number-ar.2;

            # U+0601 Year sign
        pos number-ar @DigitsAnyMedium' <-1327 0 -667 0>;
        pos @DigitsAnyMedium' <-1327 0 -667 0> number-ar;

            pos year-ar.4 @DigitsAnyMedium' <-3116 0 -667 0> @DigitsAnyMedium' <-2449 0 -667 0> @DigitsAnyMedium' <-1782 0 -667 0> @DigitsAnyMedium' <-1115 0 -667 0>;
        pos @DigitsAnyMedium' <-1115 0 -667 0> @DigitsAnyMedium' <-1782 0 -667 0> @DigitsAnyMedium' <-2449 0 -667 0> @DigitsAnyMedium' <-3116 0 -667 0> year-ar.4;

            pos year-ar.3 @DigitsAnyMedium' <-2413 0 -667 0> @DigitsAnyMedium' <-1746 0 -667 0> @DigitsAnyMedium' <-1079 0 -667 0>;
        pos @DigitsAnyMedium' <-1079 0 -667 0> @DigitsAnyMedium' <-1746 0 -667 0> @DigitsAnyMedium' <-2413 0 -667 0> year-ar.3;

            pos year-ar.2 @DigitsAnyMedium' <-1731 0 -667 0> @DigitsAnyMedium' <-1064 0 -667 0>;
        pos @DigitsAnyMedium' <-1064 0 -667 0> @DigitsAnyMedium' <-1731 0 -667 0> year-ar.2;

            # U+0602 Footnote Marker
        pos year-ar @DigitsAnyMedium' <-1065 0 -667 0>;
        pos @DigitsAnyMedium' <-1065 0 -667 0> year-ar;

            pos footnotemarker-ar.2 @DigitsAnyMedium' <-1512 0 -667 0> @DigitsAnyMedium' <-845 0 -667 0>;
        pos @DigitsAnyMedium' <-845 0 -667 0> @DigitsAnyMedium' <-1512 0 -667 0> footnotemarker-ar.2;

            # U+0603 Page Number
        pos footnotemarker-ar @DigitsAnyMedium' <-918 0 -667 0>;
        pos @DigitsAnyMedium' <-918 0 -667 0> footnotemarker-ar;

            pos pagenumber-ar.4 @DigitsAnyMedium' <-4038 0 -667 0> @DigitsAnyMedium' <-3371 0 -667 0> @DigitsAnyMedium' <-2704 0 -667 0> @DigitsAnyMedium' <-2037 0 -667 0>;
        pos @DigitsAnyMedium' <-2037 0 -667 0> @DigitsAnyMedium' <-2704 0 -667 0> @DigitsAnyMedium' <-3371 0 -667 0> @DigitsAnyMedium' <-4038 0 -667 0> pagenumber-ar.4;

            pos pagenumber-ar.3 @DigitsAnyMedium' <-3308 0 -667 0> @DigitsAnyMedium' <-2641 0 -667 0> @DigitsAnyMedium' <-1974 0 -667 0>;
        pos @DigitsAnyMedium' <-1974 0 -667 0> @DigitsAnyMedium' <-2641 0 -667 0> @DigitsAnyMedium' <-3308 0 -667 0> pagenumber-ar.3;

            pos pagenumber-ar.2 @DigitsAnyMedium' <-2573 0 -667 0> @DigitsAnyMedium' <-1906 0 -667 0>;
        pos @DigitsAnyMedium' <-1906 0 -667 0> @DigitsAnyMedium' <-2573 0 -667 0> pagenumber-ar.2;

            # U+0604 Samvat sign
        pos pagenumber-ar @DigitsAnyMedium' <-1877 0 -667 0>;
        pos @DigitsAnyMedium' <-1877 0 -667 0> pagenumber-ar;

            pos samvat-ar.4 @DigitsAnyMedium' <-4107 0 -667 0> @DigitsAnyMedium' <-3440 0 -667 0> @DigitsAnyMedium' <-2773 0 -667 0> @DigitsAnyMedium' <-2106 0 -667 0>;
        pos @DigitsAnyMedium' <-2106 0 -667 0> @DigitsAnyMedium' <-2773 0 -667 0> @DigitsAnyMedium' <-3440 0 -667 0> @DigitsAnyMedium' <-4107 0 -667 0> samvat-ar.4;

            pos samvat-ar.3 @DigitsAnyMedium' <-3457 0 -667 0> @DigitsAnyMedium' <-2790 0 -667 0> @DigitsAnyMedium' <-2123 0 -667 0>;
        pos @DigitsAnyMedium' <-2123 0 -667 0> @DigitsAnyMedium' <-2790 0 -667 0> @DigitsAnyMedium' <-3457 0 -667 0> samvat-ar.3;

            pos samvat-ar.2 @DigitsAnyMedium' <-2826 0 -667 0> @DigitsAnyMedium' <-2159 0 -667 0>;
        pos @DigitsAnyMedium' <-2159 0 -667 0> @DigitsAnyMedium' <-2826 0 -667 0> samvat-ar.2;

            # U+0605 Number mark
        pos samvat-ar @DigitsAnyMedium' <-2172 0 -667 0>;
        pos @DigitsAnyMedium' <-2172 0 -667 0> samvat-ar;

            pos numbermark-ar.4 @DigitsAnyMedium' <-2955 0 -667 0> @DigitsAnyMedium' <-2288 0 -667 0> @DigitsAnyMedium' <-1621 0 -667 0> @DigitsAnyMedium' <-954 0 -667 0>;
        pos @DigitsAnyMedium' <-954 0 -667 0> @DigitsAnyMedium' <-1621 0 -667 0> @DigitsAnyMedium' <-2288 0 -667 0> @DigitsAnyMedium' <-2955 0 -667 0> numbermark-ar.4;

            pos numbermark-ar.3 @DigitsAnyMedium' <-2328 0 -667 0> @DigitsAnyMedium' <-1661 0 -667 0> @DigitsAnyMedium' <-994 0 -667 0>;
        pos @DigitsAnyMedium' <-994 0 -667 0> @DigitsAnyMedium' <-1661 0 -667 0> @DigitsAnyMedium' <-2328 0 -667 0> numbermark-ar.3;

            pos numbermark-ar.2 @DigitsAnyMedium' <-1727 0 -667 0> @DigitsAnyMedium' <-1060 0 -667 0>;
        pos @DigitsAnyMedium' <-1060 0 -667 0> @DigitsAnyMedium' <-1727 0 -667 0> numbermark-ar.2;

            pos numbermark-ar @DigitsAnyMedium' <-950 0 -667 0>;
        pos @DigitsAnyMedium' <-950 0 -667 0> numbermark-ar;

} SubtendingMarks;

